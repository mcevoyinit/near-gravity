# Environment Configuration for NEARGravity Semantic Guard
# Copy this file to .env and fill in your actual values

# OpenAI API Configuration
OPENAI_API_KEY=your_openai_api_key_here

# Search Provider Configuration (Optional)
BRAVE_SEARCH_API_KEY=your_brave_search_api_key_here

# NEAR Protocol Configuration (Optional)
NEAR_ACCOUNT_ID=your_account.testnet
NEAR_PRIVATE_KEY=your_private_key_here
NEAR_NETWORK=testnet
NEAR_CONTRACT_ID=semantic-guard.testnet

# Service Configuration
SEMANTIC_THRESHOLD=0.75
MAX_SEARCH_RESULTS=10
PROCESSING_TIMEOUT=30

# Development Settings
USE_MOCK_SERVICES=true
DEMO_MODE=true
TOKENIZERS_PARALLELISM=false

# API URLs
NEXT_PUBLIC_API_URL=http://localhost:5000
API_PATH=shade-agent-api
API_PORT=3140

